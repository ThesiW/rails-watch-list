
<div class="container">
  <body>

    <div class="col-12 col-md-6 offset-md-3">
      <h1 class="mt-3 pt-3 d-flex justify-content-center">These are all my lists:</h1>
      <% @lists.each do |list| %>
      <p class="d-flex justify-content-center"><%= link_to list.name, list_path(list) %></p>
    <% end %>
      <p class="d-flex justify-content-center"><%= link_to "Create a new movie list" , new_list_path(@lists) %></p>
  </body>
</div>
